    "#덕타이핑 : 객체가 어떤 메서드나 행동을 지원하는지만 알고싶은 경우\n",
    "\n",
    "\n",
    "#예) 어떤 객체가 이터레이터를 지원하는지 알고싶을 때\n",
    "def isiterable(obj):\n",
    "    try:\n",
    "        iter(obj)\n",
    "        return True\n",
    "    except TypeError: #iterable객체가 아님\n",
    "        return False\n",
    "    \n",
    "print(isiterable('a string'))\n",
    "print(isiterable([1,2,3]))\n",
    "print(isiterable(5))"
   ]
  }
